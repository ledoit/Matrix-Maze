{
  "_comment": "Game name: 'Matrix Maze' - if changing, also update src/constants.js, Cargo.toml, package.json, and index.html",
  "productName": "Matrix Maze",
  "version": "1.0.0",
  "identifier": "com.matrixmaze.app",
  "build": {
    "beforeDevCommand": "cd .. && npm run dev",
    "beforeBuildCommand": "cd .. && npm run build",
    "devUrl": "http://localhost:1420"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Matrix Maze",
        "width": 1200,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all"
  },
  "plugins": {}
}
